        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrainFuck"
        mc:Ignorable="d"
      Background="White"
        Height="450" Width="800" Loaded="Window_Loaded">
    <Grid.RowDefinitions>
        <RowDefinition/>
        <RowDefinition/>
    </Grid.RowDefinitions>
    <StackPanel x:Name="valuesPanel" Grid.Row="0" Orientation="Horizontal">
    </StackPanel>
    <TextBlock x:Name="output" HorizontalAlignment="Stretch" Grid.Row="1" TextWrapping="Wrap" Text="" VerticalAlignment="Stretch" />
</Grid>
